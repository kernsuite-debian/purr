#!/usr/bin/make -f
# -*- makefile -*-
#
# Uncomment this to turn on verbose mode.
export DH_VERBOSE=1

%:
	dh $@ --with python3 --buildsystem=pybuild --parallel

override_dh_install:
	dh_install
	# remove files we don't want to install
	find $(CURDIR)/debian/tmp/ -name '*egg-info' -exec rm -r '{}' ';'


# Don't run tests
override_dh_auto_test:
